{
  "name": "growinvoice-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "zenstack": {
    "schema": "zmodel/schema.zmodel",
    "output": "shared/models"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"shared/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn run build && pm2 start dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "generate": "rimraf shared/models && zenstack generate --schema zmodel/schema.zmodel",
    "make:migration": "node make-migration.js",
    "make:migration:dev": "zenstack format && yarn run generate && yarn run migrate",
    "migrate": "prisma format && npx prisma generate && prisma migrate dev",
    "seed": "ts-node prisma/seeder/seeder.ts"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.23.0",
    "@brakebein/prisma-generator-nestjs-dto": "^1.21.0",
    "@firebase/app": "^0.10.8",
    "@firebase/storage": "^0.12.6",
    "@google/generative-ai": "^0.14.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.14.0",
    "@types/multer": "^1.4.11",
    "@zenstackhq/runtime": "2.1.2",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "firebase-admin": "^12.3.1",
    "heroku-keep-awake": "^1.1.2",
    "maskdata": "^1.3.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "nestjs-razorpay": "^2.0.1",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.14.0",
    "prisma-paginate": "^5.2.1",
    "razorpay": "^2.9.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "stripe": "^16.2.0",
    "swagger-ui-express": "^5.0.0",
    "tmp": "^0.2.3",
    "util": "^0.12.5",
    "uuidv4": "^6.2.13",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "zenstack": "2.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
